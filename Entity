if game:GetService("ReplicatedStorage").GameData.Floor.Value == "Party" then
 
entity.Config.Speed = 485
 
end

---====== Debug entity ======---
 
entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
end)
 
entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)
 
entity:SetCallback("OnReachNode", function(node)
    print("Entity has reached node\nName: ".. node.Name.. "\nPosition: ".. node.Position)
if game.Workspace.CurrentRooms[game.ReplicatedStorage.GameData.LatestRoom.Value]:GetAttribute("RawName") == "HaltHallway" then
entity.Config.Damage.Enabled = false
end
end)
 
entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime == true then
        print("Entity has entered room: ".. room.Name.. " for the first time")
    else
        print("Entity has entered room: ".. room.Name.. " again")
    end
end)
 
entity:SetCallback("OnLookAt", function(lineOfSight)
	if lineOfSight == true then
		print("Player is looking at entity")
	else
		print("Player view is obstructed by something")
	end
end)
 
entity:SetCallback("OnRebounding", function(startOfRebound)
    if startOfRebound == true then
        print("Entity has started rebounding")
	else
        print("Entity has finished rebounding")
	end
end)
 
entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
end)
 
entity:SetCallback("OnDespawned", function()
    print("Entity has despawned")
end)
 
entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
	else
		print("Entity has damaged the player")
	end
end)
 
entity:SetCallback("OnCrucified", function(stateResist)
    print("Entity was crucified")
    task.wait(3)
	if stateResist == true then
	    print("Entity is resisting the crucifixion")
	else
		print("The entity has been breaking by the crucifixion")
	end
end)
 
---====== Run entity ======---
 
entity:Run()

---====== Hiệu ứng tối màn hình tầm xa (Không sửa script trên) ======---
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local player = game:GetService("Players").LocalPlayer

-- Tạo hiệu ứng ColorCorrection chuyên biệt
local poisonCC = Lighting:FindFirstChild("CartoonCatPoison") or Instance.new("ColorCorrectionEffect")
poisonCC.Name = "CartoonCatPoison"
poisonCC.Parent = Lighting

RunService.RenderStepped:Connect(function()
    local char = player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end

    -- Tìm thực thể trong Workspace (quét rộng hơn)
    local entModel = workspace:FindFirstChild("Cartoon-cat")
    if not entModel then
        for _, v in pairs(workspace:GetChildren()) do
            if v:IsA("Model") and v.Name:find("Cartoon") and not game:GetService("Players"):GetPlayerFromCharacter(v) then
                entModel = v
                break
            end
        end
    end

    local isHiding = char:GetAttribute("Hiding") == true
    
    if entModel and not isHiding then
        local targetPart = entModel.PrimaryPart or entModel:FindFirstChildWhichIsA("BasePart")
        if targetPart then
            local dist = (char.HumanoidRootPart.Position - targetPart.Position).Magnitude
            local maxDist = 150 -- HIỆU ỨNG BẮT ĐẦU TỪ RẤT XA
            
            if dist < maxDist then
                -- Tính toán val: 0 (xa) đến 1 (sát bên)
                local val = math.clamp((maxDist - dist) / (maxDist - 10), 0, 1)
                poisonCC.Brightness = -val * 0.95 -- Gần như đen đặc khi sát bên
                poisonCC.Saturation = -val * 1.5  -- Mất màu mạnh
                poisonCC.Contrast = val * 0.5     -- Tăng độ tương phản để trông đáng sợ hơn
            else
                poisonCC.Brightness = math.lerp(poisonCC.Brightness, 0, 0.05)
                poisonCC.Saturation = math.lerp(poisonCC.Saturation, 0, 0.05)
                poisonCC.Contrast = math.lerp(poisonCC.Contrast, 0, 0.05)
            end
        end
    else
        -- Sáng lại cực mượt khi trốn hoặc thực thể đi khuất
        poisonCC.Brightness = math.lerp(poisonCC.Brightness, 0, 0.08)
        poisonCC.Saturation = math.lerp(poisonCC.Saturation, 0, 0.08)
        poisonCC.Contrast = math.lerp(poisonCC.Contrast, 0, 0.08)
    end
end)
