local cue2 = Instance.new("Sound")
	cue2.Parent = game.Workspace
	cue2.Name = "Spawn"
	cue2.SoundId = "rbxassetid://6996161849"
	cue2.Volume = 5
	cue2.PlaybackSpeed = 1
	cue2:Play()
---====== Load spawner ======---
 
